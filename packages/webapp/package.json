{
  "name": "webapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ../../build/webapp",
    "start": "webpack serve --config ./webpack/webpack.dev.js",
    "lint": "eslint -c .eslintrc.js \"**/*.{ts,tsx}\"",
    "lint:ts": "eslint -c .eslintrc.js \"/src/main\"",
    "lint:css": "stylelint \"src/**/*.ts\"",
    "build": "npm run clean && webpack --config ./webpack/webpack.prod.js",
    "build:local": "npm run clean && DEPLOYMENT_URL=http://localhost:8080 webpack --config ./webpack/webpack.prod.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.7",
    "@sentry/react": "8.38.0",
    "bootstrap": "5.3.3",
    "grapesjs": "^0.22.13",
    "grapesjs-blocks-basic": "^1.0.2",
    "grapesjs-custom-code": "^1.0.1",
    "grapesjs-navbar": "^1.0.2",
    "grapesjs-plugin-forms": "^2.0.6",
    "grapesjs-preset-webpage": "^1.0.3",
    "grapesjs-style-bg": "^2.0.1",
    "grapesjs-tabs": "^1.0.6",
    "grapesjs-tooltip": "^0.1.8",
    "grapesjs-tui-image-editor": "^1.0.2",
    "highlight.js": "^11.10.0",
    "posthog-js": "1.186.3",
    "react": "18.2.0",
    "react-bootstrap": "2.10.5",
    "react-bootstrap-icons": "1.11.4",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.30.3",
    "shared": "*",
    "styled-components": "6.1.13",
    "stylis": "4.3.4",
    "uuid": "9.0.1",
    "webpack-dev-server": "5.2.3",
    "websocket": "1.0.35"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@svgr/webpack": "8.1.0",
    "@types/react": "18.2.79",
    "@types/react-bootstrap": "0.32.37",
    "@types/react-dom": "18.2.25",
    "@types/react-redux": "^7.1.34",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "10.0.0",
    "@types/websocket": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "circular-dependency-plugin": "5.2.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.11.0",
    "ejs-loader": "0.5.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.3",
    "react-toastify": "9.1.3",
    "rimraf": "6.0.1",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
